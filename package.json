{
  "name": "react-xo-ava-app",
  "version": "1.0.0",
  "description": "ejectOf create react app with xo ava and more",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lxghtless/react-xo-ava-app.git"
  },
  "keywords": [
    "react",
    "xo",
    "ava",
    "eject"
  ],
  "author": "lxghtless",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lxghtless/react-xo-ava-app/issues"
  },
  "homepage": "https://github.com/lxghtless/react-xo-ava-app#readme",
  "dependencies": {
    "@babel/core": "7.4.3",
    "@babel/register": "^7.4.4",
    "@svgr/webpack": "4.1.0",
    "ava": "^1.4.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^8.0.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "2.1.1",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint-config-xo-react": "^0.19.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "loader-fs-cache": "^1.0.2",
    "loader-utils": "^1.2.3",
    "mini-css-extract-plugin": "0.5.0",
    "nyc": "^14.0.0",
    "object-assign": "^4.1.1",
    "object-hash": "^1.3.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.8.6",
    "react-addons-test-utils": "^15.6.2",
    "react-app-polyfill": "^1.0.0",
    "react-dev-utils": "^9.0.0",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "resolve": "1.10.0",
    "semver": "6.0.0",
    "style-loader": "0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "1.2.3",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.2.0",
    "xo": "^0.24.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "xo && NODE_ENV=test nyc ava"
  },
  "xo": {
    "ignores": [
      "config/**/*.js",
      "scripts/**/*.js"
    ],
    "envs": [
      "browser",
      "node",
      "ava"
    ],
    "extends": "xo-react",
    "plugins": [
      "unicorn",
      "react"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ]
    },
    "parser": "babel-eslint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "sources": [
      "src/**/*.js"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": true,
    "verbose": true,
    "require": [
      "./scripts/test.js"
    ]
  }
}
